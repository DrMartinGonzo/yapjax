{
  "name": "yapjax",
  "version": "1.0.0",
  "description": "Yet Another PJAX library",
  "main": "lib/index.js",
  "scripts": {
    "test": "$(npm bin)/babel-node ./node_modules/mocha/bin/_mocha",
    "cover": "$(npm bin)/babel-node ./node_modules/isparta/bin/isparta cover --report lcov --report html ./node_modules/mocha/bin/_mocha",
    "coveralls": "$(npm bin)/babel-node ./node_modules/isparta/bin/isparta cover --report lcovonly ./node_modules/mocha/bin/_mocha && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pocka/yapjax.git"
  },
  "keywords": [
    "pjax",
    "pushState",
    "ajax",
    "XMLHttpRequest"
  ],
  "author": "pocka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pocka/yapjax/issues"
  },
  "homepage": "https://github.com/pocka/yapjax#readme",
  "esformatter": {
    "indent": {
      "value": "\t"
    }
  },
  "dependencies": {
    "uid": "0.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-esformatter": "^1.1.0",
    "grunt-este-watch": "git://github.com/pocka/grunt-este-watch",
    "grunt-jsdoc": "^1.1.0",
    "ink-docstrap": "^1.1.4",
    "isparta": "^4.0.0",
    "jsdom": "^8.3.1",
    "jsdom-global": "^1.7.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3"
  }
}
